{
    "name": "atlascode",
    "displayName": "Atlassian VSCode Extensions",
    "description": "VSCode Atlassian Extensions",
    "version": "1.0.0",
    "publisher": "atlassianlabs",
    "license": "SEE LICENSE IN LICENSE",
    "homepage": "https://bitbucket.org/atlassianlabs/atlascode/src/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/atlassianlabs/atlascode.git"
    },
    "icon": "images/atlascode-icon.png",
    "preview": false,
    "galleryBanner": {
        "color": "#8647ae",
        "theme": "dark"
    },
    "keywords": [
        "bitbucket",
        "gitlens",
        "git",
        "blame",
        "log",
        "annotation",
        "multi-root ready"
    ],
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "extensionDependencies": [
        "vscode.git"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "atlascode.bb.authenticate",
                "title": "Authenticate With Bitbucket",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.bb.me",
                "title": "Get Current Bitbucket User",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.jira.me",
                "title": "Get Current Jira User",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.jira.selectProject",
                "title": "Select Working Jira Project",
                "icon": "resources/atlassian-icon.svg",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.jira.refreshExplorer",
                "title": "Refresh",
                "icon": {
                    "dark": "resources/dark/refresh.svg",
                    "light": "resources/light/refresh.svg"
                },
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.jira.authenticate",
                "title": "Authenticate With Jira",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.jira.showIssue",
                "title": "Show Jira Issue",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.jira.showIssueByKey",
                "title": "Show Jira Issue",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.bb.fetchPullRequests",
                "title": "Fetch Bitbucket pull requests",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.bb.refreshPullRequests",
                "title": "Refresh Bitbucket pull requests",
                "icon": {
                    "dark": "resources/dark/refresh.svg",
                    "light": "resources/light/refresh.svg"
                },
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.bb.showPullRequestDetails",
                "title": "Show Bitbucket pull request details",
                "category": "AtlasCode"
            },
            {
                "command": "atlascode.showConfigPage",
                "title": "Open Settings",
                "category": "AtlasCode"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "jira-viewer",
                    "title": "Atlascode",
                    "icon": "resources/atlassian-icon.svg"
                }
            ]
        },
        "views": {
            "scm": [
                {
                    "id": "atlascode.views.bb.pullrequestsTreeView",
                    "name": "Bitbucket pull requests"
                }
            ],
            "jira-viewer": [
                {
                    "id": "assignedIssues",
                    "name": "Your Issues"
                },
                {
                    "id": "openIssues",
                    "name": "Open Issues"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "atlascode.bb.refreshPullRequests",
                    "when": "view == atlascode.views.bb.pullrequestsTreeView",
                    "group": "navigation"
                },
                {
                    "command": "atlascode.jira.selectProject",
                    "when": "view =~ /(assignedIssues|openIssues)/"
                },
                {
                    "command": "atlascode.jira.refreshExplorer",
                    "when": "view =~ /(assignedIssues|openIssues)/",
                    "group": "navigation"
                }
            ],
            "view/item/context": []
        },
        "configuration": {
            "type": "object",
            "title": "AtlasCode",
            "properties": {
                "atlascode.outputLevel": {
                    "type": "string",
                    "default": "silent",
                    "enum": [
                        "silent",
                        "errors",
                        "info",
                        "debug"
                    ],
                    "description": "Specifies how much (if any) output will be sent to the AtlasCode output channel",
                    "scope": "window"
                },
                "atlascode.enableCharles": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enables proxying requests through the Charles Web Debugger when debugging",
                    "scope": "window"
                },
                "atlascode.jira.workingProject": {
                    "type": "string",
                    "default": "",
                    "description": "Jira project for workspace",
                    "scope": "resource"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "react-scripts-ts build && tsc -p tsconfig.extension.json",
        "watch": "tsc -watch -p tsconfig.extension.json",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "@atlaskit/avatar": "^14.1.2",
        "@atlaskit/avatar-group": "^2.1.5",
        "@atlaskit/button": "^9.0.16",
        "@atlaskit/comment": "^6.0.23",
        "@atlaskit/css-reset": "^3.0.2",
        "@atlaskit/page": "^8.0.8",
        "@atlaskit/reduced-ui-pack": "^10.1.1",
        "@atlaskit/table-tree": "^4.1.11",
        "ajv": "^6.5.4",
        "ajv-keywords": "^3.2.0",
        "bitbucket": "^1.5.0",
        "express": "^4.16.3",
        "git-url-parse": "^10.1.0",
        "jira": "file:../node-jira",
        "keytar": "^4.3.0",
        "parse-diff": "^0.5.1",
        "passport": "^0.4.0",
        "passport-atlassian-oauth2": "^1.0.0",
        "passport-bitbucket-oauth2": "^0.1.2",
        "passport-oauth2": "^1.4.0",
        "passport-oauth2-refresh": "^1.1.0",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-scripts-ts": "2.16.0",
        "styled-components": "^3.2.6",
        "tunnel": "^0.0.6",
        "handlebars": "^4.0.12",
        "react-loadable": "^5.5.0",
        "flatted":"^2.0.0"
    },
    "devDependencies": {
        "@types/atlaskit__button": "^6.4.1",
        "@types/git-url-parse": "^9.0.0",
        "@types/keytar": "^4.0.1",
        "@types/mocha": "^5.2.2",
        "@types/node": "^8.10.25",
        "@types/passport": "^0.4.6",
        "@types/passport-oauth2": "^1.4.6",
        "@types/react": "^16.3.2",
        "@types/react-dom": "^16.0.5",
        "@types/handlebars": "^4.0.39",
        "@types/react-loadable": "^5.4.1",
        "mocha": "^5.2.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    }
}
